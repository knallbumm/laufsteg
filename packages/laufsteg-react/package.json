{
  "name": "laufsteg-react",
  "description": "Simple infinite moving component",
  "version": "0.0.9",
  "type": "module",
  "files": [
    "dist"
  ],
  "author": "Luis Haitzer<https://github.com/quintus-luis>",
  "repository": {
    "url": "https://github.com/knallbumm/laufsteg"
  },
  "homepage": "https://github.com/knallbumm/laufsteg#readme",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.mjs",
    "default": "./dist/index.mjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "unpkg": "./dist/index.iife.min.js",
  "jsdelivr": "./dist/index.iife.min.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "dev": "vite --host",
    "build": "rimraf dist/ && rollup -c",
    "lint": "eslint . --ext .ts",
    "lint:files": "eslint --ext .ts --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit -p ."
  },
  "dependencies": {
    "@types/react": "^18.0.28",
    "@vitejs/plugin-react": "^3.1.0",
    "laufsteg": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "4.1.4"
  },
  "devDependencies": {
    "@types/react-dom": "^18.0.11"
  }
}
