{
  "name": "laufsteg-mono",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm -C ./demo/vanilla run dev ",
    "dev:react": "pnpm -C ./demo/react run dev",
    "dev:vue": "pnpm -C ./demo/vue run dev ",
    "dev:clean": "(cd ./demo && pnpm run dev)",
    "docs": "turbo dev --filter=@laufsteg/docs",
    "build": "turbo build --filter=!@laufsteg/docs",
    "build:docs": "turbo build --filter=@laufsteg/docs",
    "lint": "turbo lint",
    "lint:fix": "turbo lint",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "changeset": "changeset",
    "release": "pnpm build && changeset publish"
  },
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@laufsteg/shared": "workspace:*",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-strip": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitest/coverage-c8": "^0.28.3",
    "esbuild": "^0.17.5",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "rollup": "^3.12.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "turbo": "^1.8.3",
    "typescript": "^4.9.4",
    "vite": "4.1.4",
    "vitest": "^0.28.3"
  }
}
